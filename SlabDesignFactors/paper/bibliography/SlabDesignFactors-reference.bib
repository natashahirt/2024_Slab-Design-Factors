}

@article{mayencourtHybridAnalyticalComputational2020,
  title = {Hybrid Analytical and Computational Optimization Methodology for Structural Shaping: {{Material-efficient}} Mass Timber Beams},
  shorttitle = {Hybrid Analytical and Computational Optimization Methodology for Structural Shaping},
  author = {Mayencourt, Paul and Mueller, Caitlin},
