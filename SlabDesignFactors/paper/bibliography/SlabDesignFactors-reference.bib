@article{amirMeshbasedTopologyShape2024,
  title = {Mesh-Based Topology, Shape and Sizing Optimization of Ribbed Plates},
  author = {Amir, Oded and Majdouba, Ahmad},
  year = {2024},
  month = jun,
  journal = {Structural and Multidisciplinary Optimization},
